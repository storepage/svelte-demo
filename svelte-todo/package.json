{
  "name": "svelte-todo",
  "version": "1.0.0",
  "scripts": {
    "autobuild": "rollup -c -w",
    "build": "rollup -c",
    "dev": "run-p start:dev autobuild",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest --watch src"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@testing-library/svelte": "^1.7.0",
    "babel-jest": "^24.8.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-svelte3": "^1.2.3",
    "jest": "^24.8.0",
    "jest-transform-svelte": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.6.0",
    "rollup": "^1.14.5",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.4.4"
  }
}
